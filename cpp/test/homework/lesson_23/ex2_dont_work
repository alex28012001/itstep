#include<iostream>
#include<cmath>
using namespace std;
int sqrt(int a)
{
	return sqrt(a);
}
int kub(int a)
{
	return a*a*a;
}
int sin(int a)
{
	return sin(a);
}

void print(int a, int(*alg)(int))
{
	cout << (*alg)(a);
}
double(*getfunptr())(double,double)
{
	double(*ptr)(double, double) = NULL;
	int choice;
	cout << "\n1.+\n2.-\n3.*\n4. /\n";
	cin >> choice;
	


	return ptr;
}
